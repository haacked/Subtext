        {blog-homepage-url}admin/EditPosts.aspx
    ]]></postEditingUrl>
    <watermarkImageUrl>images/subtextWatermark.png</watermarkImageUrl>
    <imageUrl>images/subtextLogo32-23.png</imageUrl>
  </weblog>
</manifest>